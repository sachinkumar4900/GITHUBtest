            "binding": {
              "config": {
                "path": "../PDFViewer.props.source"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "target": "tab",
          "text": "View Document in Separate Window"
        },
        "type": "ia.navigation.link"
      },
      {
        "meta": {
          "name": "Label"
        },
        "position": {
