    "custom.OEE": {
      "binding": {
        "config": {
          "expression": "now(60000)"
        },
        "transforms": [
          {
            "code": "\tsite \u003d self.session.custom.ProductionSite\n\toffset \u003d -7 if site \u003d\u003d \"Moorpark\" else -4\n\tnow \u003d system.db.runNamedQuery(\u0027Downtime/GetUTC\u0027)[0][0]\n\ttagPath \u003d self.custom.tagPath\n\t\n\tstartObj \u003d system.date.fromMillis(self.params.StartTime) if self.params.StartTime is not None else None\n\tendObj \u003d now\n\tstart \u003d system.date.addHours(system.date.parse(startObj), offset) if self.params.StartTime is not None else None\n\tend \u003d system.date.addHours(system.date.parse(endObj), offset) if endObj is not None else None\n#\tsystem.perspective.print(\u0027tagPath: \u0027 + tagPath + \u0027 ---- start: \u0027 + str(start) + \u0027 ---- end: \u0027 + str(end))\n\t\n\tavail \u003d OEE.getAvailability(tagPath, start, end)\n\tperf \u003d OEE.getPerformance(tagPath, start, end)\n\tqual \u003d OEE.getAndonQuality(tagPath, start, end)\n\toee \u003d avail * perf * qual\n\t\t\n\tandon \u003d system.tag.readBlocking([tagPath + \"/OEE/Quality/Work Order Count\", tagPath + \"/OEE/Quality/Scrap Count\"])\n\ttotalCount \u003d andon[0].value if andon[0].value is not None else 0\n\tscrapCount \u003d andon[1].value if andon[1].value is not None else 0\n\n\t\t#system.perspective.print(\u0027 ----------- \u0027 + statusTime)\n\tds \u003d ({\n\t\t\t\"Availability\":avail,\n\t\t\t\"Performance\":perf,\n\t\t\t\"Quality\":qual,\n\t\t\t\"OEE\":oee,\n\t\t\t\"GoodCount\":totalCount - scrapCount,\n\t\t\t})\n\treturn ds",
            "type": "script"
          }
        ],
        "type": "expr"
      },
      "persistent": true
    },
