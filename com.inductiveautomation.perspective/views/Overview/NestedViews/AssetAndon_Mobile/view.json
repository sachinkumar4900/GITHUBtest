{
  "custom": {
    "EquipmentTag": "value"
  },
  "params": {
    "ActualCount": "value",
    "Asset": "SB01",
    "TotalCount": "value",
    "WorkOrder": "value"
  },
  "propConfig": {
    "custom.EquipmentTag": {
      "persistent": true
    },
    "params.ActualCount": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.Asset": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.TotalCount": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.WorkOrder": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 150,
      "width": 400
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Asset_Label"
        },
        "position": {
          "height": 0.3279,
          "width": 0.485,
          "x": 0.025,
          "y": 0.012
        },
        "propConfig": {
          "props.style.color": {
            "binding": {
              "config": {
                "expression": "if({../Status_Label.props.text} \u003d \u0027running\u0027, \u0027#D5D5D5\u0027, \r\nif({../Status_Label.props.text} \u003d \u0027stopped\u0027, \u0027#D5D5D5\u0027,\r\nif({../Status_Label.props.text} \u003d \u0027not scheduled\u0027, \u0027#D5D5D5\u0027,\r\n\u0027#000000\u0027)))"
              },
              "type": "expr"
            }
          },
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.Asset"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "textStyle": {
            "classes": "Mobile/Header2"
          }
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "Status_Label"
        },
        "position": {
          "height": 0.264,
          "width": 0.6625,
          "x": 0.025,
          "y": 0.2719
        },
        "propConfig": {
          "props.style.color": {
            "binding": {
              "config": {
                "expression": "if({this.props.text} \u003d \u0027running\u0027, \u0027#D5D5D5\u0027, \r\nif({this.props.text} \u003d \u0027stopped\u0027, \u0027#D5D5D5\u0027,\r\nif({this.props.text} \u003d \u0027not scheduled\u0027, \u0027#D5D5D5\u0027,\r\n\u0027#000000\u0027)))"
              },
              "type": "expr"
            }
          },
          "props.text": {
            "binding": {
              "config": {
                "fallbackDelay": 2.5,
                "mode": "indirect",
                "references": {
                  "Asset": "{view.params.Asset}"
                },
                "tagPath": "[default]Molding/{Asset}/Status"
              },
              "type": "tag"
            }
          }
        },
        "props": {
          "textStyle": {
            "classes": "Mobile/Text_Large"
          }
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "height": 0.28,
          "width": 0.365,
          "x": 0.6075,
          "y": 0.036
        },
        "propConfig": {
          "custom.StatusTime": {
            "binding": {
              "config": {
                "fallbackDelay": 2.5,
                "mode": "indirect",
                "references": {
                  "Asset": "{view.params.Asset}"
                },
                "tagPath": "[default]Molding/{Asset}/Andon/TimeStamp"
              },
              "type": "tag"
            }
          },
          "props.style.color": {
            "binding": {
              "config": {
                "expression": "if({../Status_Label.props.text} \u003d \u0027running\u0027, \u0027#D5D5D5\u0027, \r\nif({../Status_Label.props.text} \u003d \u0027stopped\u0027, \u0027#D5D5D5\u0027,\r\nif({../Status_Label.props.text} \u003d \u0027not scheduled\u0027, \u0027#D5D5D5\u0027,\r\n\u0027#000000\u0027)))"
              },
              "type": "expr"
            }
          },
          "props.text": {
            "binding": {
              "config": {
                "expression": "if(\r\n\tisNull({this.custom.StatusTime}),\r\n\t\"0:00:00\",\r\n\tstringFormat(\r\n\t\t\"%02d:%02d:%02d\",\r\n\t\ttoInt(floor(dateDiff({this.custom.StatusTime},now(),\"second\")/86400)),\r\n\t\ttoInt(floor(dateDiff({this.custom.StatusTime},now(),\"second\")%86400/3600)),\r\n\t\ttoInt(floor(dateDiff({this.custom.StatusTime},now(),\"second\")%3600/60))\r\n\t)\r\n)"
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "textStyle": {
            "classes": "Mobile/Text_Large",
            "textAlign": "right"
          }
        },
        "type": "ia.display.label"
      },
      {
        "children": [
          {
            "meta": {
              "name": "Status_Label"
            },
            "position": {
              "height": 0.4089,
              "width": 0.865,
              "x": 0.0675,
              "y": 0.1016
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "expression": "\"Work Order: \" + {view.params.WorkOrder}"
                  },
                  "type": "expr"
                }
              }
            },
            "props": {
              "style": {
                "color": "#D5D5D5",
                "textAlign": "center"
              },
              "textStyle": {
                "classes": "Mobile/Text_Large"
              }
            },
            "type": "ia.display.label"
          },
          {
            "meta": {
              "name": "Status_Label_0"
            },
            "position": {
              "height": 0.4089,
              "width": 0.7725,
              "x": 0.1138,
              "y": 0.5967
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "expression": "{view.params.ActualCount} + \" of \" + {view.params.TotalCount}"
                  },
                  "type": "expr"
                }
              }
            },
            "props": {
              "style": {
                "color": "#D5D5D5",
                "textAlign": "center"
              }
            },
            "type": "ia.display.label"
          }
        ],
        "meta": {
          "name": "CoordinateContainer"
        },
        "position": {
          "height": 0.4133,
          "width": 1,
          "y": 0.5837
        },
        "props": {
          "mode": "percent",
          "style": {
            "backgroundColor": "#505050"
          }
        },
        "type": "ia.container.coord"
      }
    ],
    "meta": {
      "name": "root"
    },
    "propConfig": {
      "custom.TagAndonGroup": {
        "binding": {
          "config": {
            "fallbackDelay": 2.5,
            "mode": "indirect",
            "references": {
              "Asset": "{view.params.Asset}"
            },
            "tagPath": "[default]Molding/{Asset}/Andon/Andon Group"
          },
          "type": "tag"
        }
      },
      "props.style.backgroundColor": {
        "binding": {
          "config": {
            "expression": "if({./Status_Label.props.text} \u003d \u0027running\u0027 \u0026\u0026 {this.custom.TagAndonGroup} \u003d -1, \u0027#228B22\u0027,\r\n\tif({./Status_Label.props.text} \u003d \u0027running\u0027 \u0026\u0026 {this.custom.TagAndonGroup} !\u003d -1, \u0027#CCCC00\u0027,\r\n\t\tif({./Status_Label.props.text} \u003d \u0027stopped\u0027, \u0027#B22222\u0027,\r\n\t\t\tif({./Status_Label.props.text} \u003d \u0027not scheduled\u0027, \u0027#006E96\u0027,\r\n\t\t\u0027#CCCC00\u0027))))"
          },
          "type": "expr"
        }
      }
    },
    "props": {
      "mode": "percent",
      "style": {
        "borderBottomLeftRadius": "8px",
        "borderBottomRightRadius": "8px",
        "borderTopLeftRadius": "8px",
        "borderTopRightRadius": "8px",
        "marginBottom": 0,
        "marginTop": "0.5rem"
      }
    },
    "type": "ia.container.coord"
  }
}