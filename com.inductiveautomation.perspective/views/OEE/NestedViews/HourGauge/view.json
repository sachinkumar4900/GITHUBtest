        "props": {
          "style": {
            "borderColor": "#000000",
            "borderStyle": "solid",
            "borderWidth": "1px",
            "classes": "Card",
            "fontStyle": "italic",
            "fontWeight": "bolder",
            "textAlign": "center"
          }
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "Time Value"
        },
        "position": {
          "height": 22,
          "width": "100%",
          "y": 5
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.Hour"
              },
              "type": "property"
            }
