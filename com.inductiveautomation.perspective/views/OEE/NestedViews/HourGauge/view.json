              "config": {
                "path": "view.params.Count"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "fineTicks": {
            "color": "#000000",
            "span": 0
          },
          "indicators": [
            {
              "color": "#D8D8D8",
              "extent": 105000,
              "indicatorStyle": "range",
              "label": "",
              "labelAngle": 0,
              "labelColor": "#000000",
              "length": 40,
              "opacity": 1,
              "stroke": 1,
              "value": 0
            },
            {
              "indicatorStyle": "range",
              "label": "",
              "labelAngle": 270,
