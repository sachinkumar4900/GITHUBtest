          "y": 52
        },
        "propConfig": {
          "props.indicators[1].color": {
            "binding": {
              "config": {
                "expression": "if({view.params.Perf} \u003c 71, \u0027#C21D4F\u0027, \u0027#6BA364\u0027)"
              },
              "type": "expr"
            }
          },
          "props.indicators[1].extent": {
