{
  "custom": {},
  "params": {
    "Count": "value",
    "Hour": "value",
    "Perf": "value",
    "Qual": "value",
    "Rejects": "value"
  },
  "propConfig": {
    "params.Count": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.Hour": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.Perf": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.Qual": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.Rejects": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 362,
      "width": 240
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Quality Value"
        },
        "position": {
          "height": 22,
          "width": "100%",
          "y": 340
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.Qual"
              },
              "transforms": [
                {
                  "formatType": "numeric",
                  "formatValue": "##0.00",
                  "type": "format"
                },
                {
                  "expression": "concat({value}, \u0027% Quality\u0027)",
                  "type": "expression"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Card",
            "textAlign": "center"
          }
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "Rejects Value"
        },
        "position": {
          "height": 21,
          "width": "100%",
          "y": 320
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "expression": "concat({view.params.Rejects}, \" Rejects\")"
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Card",
            "textAlign": "center"
          }
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "Eff Value"
        },
        "position": {
          "height": 22,
          "width": "100%",
          "y": 25
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.Perf"
              },
              "transforms": [
                {
                  "formatType": "numeric",
                  "formatValue": "##0.00",
                  "type": "format"
                },
                {
                  "expression": "concat(round({value}, 1), \u0027% Performance\u0027)",
                  "type": "expression"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Card",
            "fontWeight": "normal",
            "textAlign": "center"
          }
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "Count Value"
        },
        "position": {
          "height": 22,
          "width": "50%",
          "x": 60,
          "y": 285
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.Count"
              },
              "type": "property"
            }
          }
