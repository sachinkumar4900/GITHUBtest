{
  "custom": {},
  "params": {
    "icon": "material/dashboard",
    "link": "",
    "name": "Dashboard"
  },
  "propConfig": {
    "params.icon": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.link": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.name": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 67,
      "width": 300
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Icon"
        },
        "position": {
          "basis": "29px"
        },
        "propConfig": {
          "props.path": {
            "binding": {
              "config": {
                "path": "view.params.icon"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "color": "#008ABC"
          }
        },
        "type": "ia.display.icon"
      },
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "basis": "261.2px"
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.name"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Barlow",
            "fontSize": "1.5rem"
          },
          "textStyle": {
            "classes": "Barlow",
            "fontWeight": "bold"
          }
        },
        "type": "ia.display.label"
      }
    ],
    "events": {
      "dom": {
        "onClick": {
          "config": {
            "page": "{view.params.link}"
          },
          "scope": "C",
          "type": "nav"
        }
      }
    },
    "meta": {
      "name": "root"
    },
    "props": {
      "style": {
        "borderStyle": "none",
        "gap": "8px"
      }
    },
    "type": "ia.container.flex"
  }
}