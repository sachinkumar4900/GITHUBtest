      {
        "events": {
          "component": {
            "onEditCellCommit": {
              "config": {
                "script": "    # Get the selected row\u0027s data\n    selectedRow \u003d self.props.selection.data[0]\n\n    # Initialize parameters for the named query\n    Param \u003d {\n        \"ID\": selectedRow[\"ID\"],\n        \"FPY_Target\": selectedRow[\"FPY_Target\"],\n        \"OTD_Target\": selectedRow[\"OTD_Target\"]\n    }\n\n    # Check which column was edited and update the relevant parameter\n    if event.column \u003d\u003d \"FPY_Target\":\n        Param[\"FPY_Target\"] \u003d event.value  # Update FPY_Target with the new value\n    elif event.column \u003d\u003d \"OTD_Target\":\n        Param[\"OTD_Target\"] \u003d event.value  # Update OTD_Target with the new value\n\n    try:\n        # Show loading spinner by setting the class or style\n        self.getSibling(\"Image\").props.style.classes \u003d \"loader-28\"\n\n        # Run the named query to update Targets\n        system.db.runNamedQuery(\"Lean_Query/UpdateTargets\", Param)\n\n    except Exception as e:\n        # Display error message\n        system.gui.errorBox(\"Error while updating: {str(e)}\")\n\n    finally:\n        # Refresh the binding to reflect updated data in the table\n        self.refreshBinding(\"props.data\")"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
