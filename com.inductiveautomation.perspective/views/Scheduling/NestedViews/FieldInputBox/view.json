{
  "custom": {},
  "params": {
    "InputValue": null,
    "value": "int"
  },
  "propConfig": {
    "params.InputValue": {
      "binding": {
        "config": {
          "expression": "if({/root/Text.position.display}, {/root/Text.props.text}, {/root/NumericEntryField.props.value})"
        },
        "type": "expr"
      },
      "paramDirection": "output",
      "persistent": true
    },
    "params.value": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 20,
      "width": 50
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Text"
        },
        "position": {
          "basis": "32px",
          "grow": 1
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "path": "view.params.value"
              },
              "transforms": [
                {
                  "expression": "{value} \u003d \"str\" || {value} \u003d \"string\" || {value} \u003d \"text\" ",
                  "type": "expression"
                }
              ],
              "type": "property"
            }
          },
          "props.placeholder": {
            "binding": {
              "config": {
                "path": "view.params.value"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "textAlign": "end"
          }
        },
        "type": "ia.input.text-field"
      },
      {
        "meta": {
          "name": "NumericEntryField"
        },
        "position": {
          "basis": "32px",
          "grow": 1
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "path": "view.params.value"
              },
              "transforms": [
                {
                  "expression": "{value} !\u003d \"str\" \u0026\u0026 {value} !\u003d \"string\" \u0026\u0026 {value} !\u003d \"text\" ",
                  "type": "expression"
                }
              ],
              "type": "property"
            }
          },
          "props.placeholder": {
            "binding": {
              "config": {
                "path": "view.params.value"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "overflow": "visible",
            "textAlign": "end"
          },
          "value": null
        },
        "type": "ia.input.numeric-entry-field"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "overflow": "hidden"
      }
    },
    "type": "ia.container.flex"
  }
}