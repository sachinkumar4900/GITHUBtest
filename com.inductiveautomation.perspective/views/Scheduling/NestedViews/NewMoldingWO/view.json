          "custom.PartId": {
            "binding": {
              "config": {
                "expression": "{../DataEntry/Fields/PartNumber/TextField.props.text} + \"-\" + \r\n{view.params.ProductionLine.Name} + \"-\" + \r\n{../DataEntry/Fields/MoldNumber/NumericEntryField.props.value} + \"-\" + \r\n{../DataEntry/Fields/ResinPartNumber/Dropdown.props.value}"
              },
              "type": "expr"
            }
          },
          "props.enabled": {
            "binding": {
              "config": {
                "expression": "{../DataEntry/Fields/PartNumber/TextField.props.text} !\u003d \"\" \u0026\u0026\r\n{../DataEntry/Fields/MoldNumber/TextField.props.value} !\u003d \"\" \u0026\u0026\r\n{../DataEntry/Fields/ResinPartNumber/TextField.props.value} !\u003d \"\" \u0026\u0026\r\n{../DataEntry/Fields/ProcessSheetLocation/TextField.props.text} !\u003d \"\" \u0026\u0026\r\n{../DataEntry/Fields/BOMWeight/TextField.props.value} !\u003d \"\" \u0026\u0026\r\n{../DataEntry/Fields/Cavities/TextField.props.value} !\u003d \"\" \u0026\u0026\r\n{../DataEntry/FieldsCycleTimeID/TextField.props.value} !\u003d \"\" \u0026\u0026\r\n{../DataEntry/Fields/ScrapRate/TextField.props.value} !\u003d \"\" \u0026\u0026\r\n{../DataEntry/Fields/ChangeoverTime/TextField.props.value} !\u003d \"\" "
              },
              "overlayOptOut": true,
              "type": "expr"
            }
          },
