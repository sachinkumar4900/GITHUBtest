          {
            "events": {
              "component": {
                "onMoveEvent": {
                  "config": {
                    "script": "\tquantity \u003d -1\n\tfor entry in self.props.scheduledEvents:\n\t\tif entry[\"eventId\"] \u003d\u003d event.eventId:\n\t\t\tquantity \u003d entry[\"quantity\"]\n\t\t\t\n\tstartDate \u003d system.date.parse(event.start)\n\tendDate \u003d system.date.parse(event.end)\n\tparams \u003d {\"ID\":event.eventId,\n\t\t\t\"StartTime\":startDate,\n\t\t\t\"EndTime\":endDate,\n\t\t\t\"Quantity\":quantity}\n\treturn system.db.runNamedQuery(\"UpdateWOScheduleEntry\", params)"
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
            },
            "meta": {
              "name": "EquipmentSchedule"
            },
            "position": {
              "basis": "750px",
              "grow": 1
            },
            "propConfig": {
              "custom.backup_1": {
                "binding": {
                  "config": {
                    "parameters": {
                      "StartTime": "{this.props.dateRange.startDate}"
                    },
                    "queryPath": "GetCurrentWOSchedule"
                  },
                  "transforms": [
                    {
                      "code": "\tscheduledEvents \u003d []\n\tfor row in range(value.getRowCount()):\n\t\n\t\tscheduledEvents.append({\n\t\t\"itemId\":value.getValueAt(row,\"ProductionLineID\"),\n\t\t\"eventId\":value.getValueAt(row,\"ID\"),\n\t\t\"startDate\":value.getValueAt(row,\"StartTime\"),\n\t\t\"endDate\":value.getValueAt(row,\"EndTime\"),\n\t\t\"label\":value.getValueAt(row,\"WorkOrderID\"),\n\t\t\"leadStyle\":{},\n\t\t\"percentDone\":0,\n\t\t\"style\":{}\n\t\t})\n\t\t\n\t\n\t\n\treturn scheduledEvents",
                      "type": "script"
                    }
                  ],
                  "type": "query"
                }
              },
              "custom.data": {
                "binding": {
                  "config": {
                    "queryPath": "GetWOScheduleAndLine"
                  },
                  "type": "query"
                }
              },
              "props.dateRange.endDate": {
                "binding": {
                  "config": {
                    "expression": "addDays(now(),1)"
                  },
                  "type": "expr"
                }
              },
              "props.dateRange.startDate": {
                "binding": {
                  "config": {
                    "expression": "now()"
                  },
                  "type": "expr"
                }
              },
              "props.items": {
                "binding": {
                  "config": {
                    "polling": {
                      "enabled": true,
                      "rate": "30"
                    },
                    "queryPath": "GetProductionLines"
                  },
                  "transforms": [
                    {
                      "code": "\tlineInfo \u003d []\n\tfor row in range(value.getRowCount()):\n\t\tlineInfo.append({\n\t\t\t\"id\":value.getValueAt(row,\"ID\"),\n\t\t\t\"label\":value.getValueAt(row,\"Name\"),\n\t\t\t\"iconConfig\":{\"path\":\"\",\"color\":\"c0c0c0\",\"style\":{}},\n\t\t\t\"headerStyle\":{},\n\t\t\t\"rowStyle\":{},\n\t\t\t\"footerStyle\":{}\n\t\t})\n\treturn lineInfo",
                      "type": "script"
                    }
                  ],
                  "type": "query"
                }
              },
              "props.scheduledEvents": {
                "binding": {
                  "config": {
                    "parameters": {
                      "StartTime": "{this.props.dateRange.startDate}"
                    },
                    "queryPath": "GetCurrentWOSchedule"
                  },
                  "transforms": [
                    {
                      "code": "\tscheduledEvents \u003d []\n\tfor row in range(value.getRowCount()):\n\t\n\t\tscheduledEvents.append({\n\t\t\"itemId\":value.getValueAt(row,\"ProductionLineID\"),\n\t\t\"eventId\":value.getValueAt(row,\"ID\"),\n\t\t\"startDate\":value.getValueAt(row,\"StartTime\"),\n\t\t\"endDate\":value.getValueAt(row,\"EndTime\"),\n\t\t\"label\":value.getValueAt(row,\"WorkOrderID\"),\n\t\t\"leadStyle\":{},\n\t\t\"percentDone\":0,\n\t\t\"style\":{},\n\t\t\"quantity\":value.getValueAt(row,\"Quantity\")\n\t\t})\n\t\t\n\t\n\t\n\treturn scheduledEvents",
                      "type": "script"
                    }
                  ],
                  "type": "query"
                }
              }
            },
            "props": {
              "dateRange": {},
              "selectedEvent": {
                "eventId": 28,
                "itemId": 1
              }
            },
