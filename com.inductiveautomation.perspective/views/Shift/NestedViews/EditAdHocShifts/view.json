                "config": {
                  "script": "\thoursEntry \u003d self.getSibling(\"HoursEntryField\").props.value + (self.getSibling(\"MinutesEntryField\").props.value/60.0)\n\tpID \t\t\t\t\u003d self.getSibling(\"Table\").props.selection.data[0].ID\n\tpName \t\t\t\t\u003d self.getSibling(\"TextField\").props.text\n\tpStartTime \t\t\t\u003d self.getSibling(\"DateTimeInput\").props.value\n\tpHours \t\t\t\t\u003d hoursEntry\n\tpProductionLineID\t\u003d self.getSibling(\"Dropdown\").props.value\n\tpDefault \t\t\t\u003d self.getSibling(\"Table\").props.selection.data[0].Default\n\tpEnabled \t\t\t\u003d 0 if self.getSibling(\"Table\").props.selection.data[0].Enabled else 1\n\tpIsDelete \t\t\t\u003d False\n\t\n\tparams \u003d {\"pID\": pID, \"pName\": pName, \"pStartTime\": pStartTime, \"pHours\": pHours, \"pProductionLineID\": pProductionLineID, \"pDefault\": pDefault, \"pEnabled\": pEnabled, \"pIsDelete\": pIsDelete}\n\tid \u003d system.db.runNamedQuery(\"Shift/sp_MD_Shift\", params)"
                },
                "scope": "G",
                "type": "script"
              },
              {
                "config": {
                  "script": "\tself.getSibling(\"Table\").refreshBinding(\"props.data\")\n\t\n\tself.getSibling(\"Table\").props.selection.selectedRow \u003d None\n\t\n\tself.getSibling(\"TextField\").props.text \u003d \u0027\u0027\n\tself.getSibling(\"DateTimeInput\").props.value \u003d \u002700:00:00\u0027\n\tself.getSibling(\"HoursEntryField\").props.value \u003d 0\n\tself.getSibling(\"MinutesEntryField\").props.value \u003d 0\n\tself.getSibling(\"Dropdown\").props.value \u003d None"
                },
                "scope": "G",
                "type": "script"
              }
            ]
          }
        },
