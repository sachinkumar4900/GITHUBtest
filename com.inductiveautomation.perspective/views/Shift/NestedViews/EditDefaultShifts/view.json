        "events": {
          "component": {
            "onActionPerformed": [
              {
                "config": {
                  "script": "\thoursEntry \u003d self.getSibling(\"HoursEntryField\").props.value + (self.getSibling(\"MinutesEntryField\").props.value/60.0)\n\tif self.getSibling(\"Table\").props.selection.selectedRow is None:\n\t\n\t\tpID \t\t\t\t\u003d None\n\t\tpName \t\t\t\t\u003d self.getSibling(\"TextField\").props.text\n\t\tpStartTime \t\t\t\u003d self.getSibling(\"DateTimeInput\").props.value\n\t\tpHours \t\t\t\t\u003d hoursEntry\n\t\tpProductionLineTypeID\t\u003d self.getSibling(\"Dropdown\").props.value\n\t\tpDefault \t\t\t\u003d True\n\t\tpEnabled \t\t\t\u003d True\n\t\tpIsDelete \t\t\t\u003d False\n\telse:\n\t\n\t\t#pID \t\t\t\t\u003d self.getSibling(\"Table\").props.selection.data[0].ShiftID\n\t\tpName \t\t\t\t\u003d self.getSibling(\"TextField\").props.text\n\t\tpStartTime \t\t\t\u003d self.getSibling(\"DateTimeInput\").props.value\n\t\tpHours \t\t\t\t\u003d hoursEntry\n\t\tpProductionLineTypeID\t\u003d self.getSibling(\"Dropdown\").props.value\n\t\tpDefault \t\t\t\u003d True\n\t\tpEnabled \t\t\t\u003d True\n\t\tpIsDelete \t\t\t\u003d False\n\t\n\tprodSiteID \u003d self.session.custom.ProductionSiteID\n\tprodLines \u003d system.db.runPrepQuery(\"SELECT ID FROM ProductionLine WHERE ProductionLineTypeID \u003d ? AND ProductionSiteID \u003d ?\", [pProductionLineTypeID, prodSiteID])\n\tfor prodLine in prodLines:\n\t\tpProductionLineID \u003d prodLine[0]\n\t\t\n\t\tpID \u003d system.db.runScalarPrepQuery(\"SELECT ID FROM Shift WHERE ProductionLineID \u003d ? AND [Name] \u003d ?\", [pProductionLineID, pName])\n\t\t\n\t\tparams \u003d {\"pID\": pID, \"pName\": pName, \"pStartTime\": pStartTime, \"pHours\": pHours, \"pProductionLineID\": pProductionLineID, \"pDefault\": pDefault, \"pEnabled\": pEnabled, \"pIsDelete\": pIsDelete}\n\t\tsystem.db.runNamedQuery(\"Shift/sp_MD_Shift\", params)\n\tsystem.db.runPrepUpdate(\u0027\u0027\u0027UPDATE ShiftSchedule SET StartTime \u003d REPLACE(convert(varchar(27), ss.StartTime, 120), SUBSTRING(convert(varchar(27), ss.StartTime, 120), 12,16), s.StartTime) ,EndTime \u003d DATEADD(hour, s.Hours, REPLACE(convert(varchar(27), ss.StartTime, 120), SUBSTRING(convert(varchar(27), ss.StartTime, 120), 12,16), s.StartTime)) FROM ShiftSchedule ss JOIN Shift s ON ss.ShiftID \u003d s.ID WHERE ss.EndTime \u003e GETDATE()\u0027\u0027\u0027)"
                },
                "scope": "G",
                "type": "script"
              },
              {
                "config": {
                  "script": "\tself.getSibling(\"Table\").refreshBinding(\"props.data\")\n\t\n\tself.getSibling(\"Table\").props.selection.selectedRow \u003d None\n\t\n\tself.getSibling(\"TextField\").props.text \u003d \u0027\u0027\n\tself.getSibling(\"DateTimeInput\").props.value \u003d \u002700:00:00\u0027\n\tself.getSibling(\"HoursEntryField\").props.value \u003d 0\n\tself.getSibling(\"MinutesEntryField\").props.value \u003d 0\n\tself.getSibling(\"Dropdown\").props.value \u003d None"
                },
                "scope": "G",
                "type": "script"
              }
            ]
          }
