      {
        "events": {
          "component": {
            "onActionPerformed": {
              "config": {
                "script": "\tid \u003d self.props.value\n\tif id !\u003d 0:\n\t\tds \u003d system.db.runPrepQuery(\"SELECT StartTime, Hours FROM Shift WHERE ID \u003d ?\", [id])\n\t\tstartTime \u003d ds[0][0]\n\t\thours \u003d ds[0][1]\n\t\t\n\t\tself.getSibling(\"Start DateTimeInput\").props.value \u003d system.date.addSeconds(system.date.addMinutes(system.date.addHours(system.date.midnight(system.date.now()), system.date.getHour24(startTime)), system.date.getMinute(startTime)), system.date.getSecond(startTime))\n\t\tself.getSibling(\"End DateTimeInput\").props.value \u003d system.date.addMinutes(self.getSibling(\"Start DateTimeInput\").props.value, int(hours * 60))"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
