          "component": {
            "onActionPerformed": [
              {
                "config": {
                  "script": "\t\n\tpID \t\t\t\t\u003d None\n\tpShiftID \t\t\t\u003d self.getSibling(\"Shift Dropdown\").props.value\n\tpStartTime \t\t\t\u003d self.getSibling(\"Start DateTimeInput\").custom.formattedDate\n\tpEndTime \t\t\t\u003d self.getSibling(\"End DateTimeInput\").custom.formattedDate\n\tpProductionLineID\t\u003d self.getSibling(\"Line Dropdown\").props.value\n\tpIsDelete \t\t\t\u003d False\n\t\n\t\n\tparams \u003d {\"pID\": pID, \"pShiftID\": pShiftID, \"pStartTime\": pStartTime, \"pEndTime\": pEndTime, \"pProductionLineID\": pProductionLineID, \"pIsDelete\": pIsDelete}\n\tsystem.db.runNamedQuery(\"Shift/sp_PLAN_ShiftSchedule\", params)"
                },
                "scope": "G",
                "type": "script"
              },
              {
                "config": {
                  "script": "\tself.getSibling(\"Table\").refreshBinding(\"props.data\")\n\t"
                },
                "scope": "G",
                "type": "script"
              }
            ]
          }
        },
        "meta": {
          "name": "Button_0"
        },
        "position": {
          "height": 0.0567,
          "width": 0.1167,
          "x": 0.3704,
          "y": 0.0386
        },
        "propConfig": {
          "props.enabled": {
            "binding": {
              "config": {
                "expression": "if(({../Shift Dropdown.props.value} !\u003d 0\r\n\u0026\u0026\r\n{parent.custom.TimeslotTaken}) || ({../Start DateTimeInput.props.value} \u003e {../End DateTimeInput.props.value}), false, true)"
              },
              "type": "expr"
            }
          },
          "props.primary": {
            "binding": {
              "config": {
                "path": "this.props.enabled"
              },
              "type": "property"
            }
          }
