        },
        "props": {
          "style": {
            "textAlign": "right"
          },
          "text": "Reason Group:"
        },
        "type": "ia.display.label"
      },
      {
        "events": {
          "component": {
            "onActionPerformed": {
              "config": {
                "script": "\tself.getSibling(\"Reason TextField\").props.text \u003d \"\"\n\tself.getSibling(\"Consecutive NumericEntryField\").props.value \u003d 0\n\tself.getSibling(\"Defects NumericEntryField\").props.value \u003d 0\n\tself.getSibling(\"Reason Group Dropdown\").props.value \u003d \"\""
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "Reset Button"
        },
        "position": {
          "height": 34,
          "width": 80,
          "x": 105.8,
          "y": 221
        },
        "props": {
          "text": "Reset"
        },
        "type": "ia.input.button"
      },
      {
        "events": {
          "component": {
            "onActionPerformed": [
              {
                "config": {
                  "script": "\tparams \u003d {\"pID\": None, \"pName\": self.getSibling(\"Reason TextField\").props.text, \"pConsecutiveDefects\": self.getSibling(\"Consecutive NumericEntryField\").props.value, \"pDefectsPerHour\": self.getSibling(\"Defects NumericEntryField\").props.value, \"pProductionLineTypeID\": self.getSibling(\"Reason Group Dropdown\").props.value, \"pIsDelete\": False}\n\tsystem.db.runNamedQuery(\"Scrap/sp_MD_ScrapReason\", params)"
                },
                "scope": "G",
                "type": "script"
              },
              {
                "config": {
                  "script": "\tsystem.perspective.sendMessage(\"refresh-Scrap-Reason-Table\", {})\n\tsystem.perspective.closePopup(\u0027\u0027)"
                },
                "scope": "G",
                "type": "script"
              }
            ]
          }
        },
        "meta": {
          "name": "Save Button"
        },
        "position": {
          "height": 34,
          "width": 80,
          "x": 273.29,
          "y": 221
        },
        "propConfig": {
          "custom.DefectsSet": {
            "binding": {
              "config": {
                "expression": "{../Defects NumericEntryField.props.value} \u003e 0\r\n\u0026\u0026\r\n{../Consecutive NumericEntryField.props.value} \u003e 0"
              },
              "type": "expr"
            }
          },
          "custom.ReasonGroupSet": {
            "binding": {
              "config": {
                "expression": "{../Reason Group Dropdown.props.value} !\u003d \"\""
              },
              "type": "expr"
            }
          },
          "custom.ReasonSet": {
            "binding": {
              "config": {
                "expression": "len({../Reason TextField.props.text}) \u003e 0"
              },
              "type": "expr"
            }
          },
          "props.enabled": {
            "binding": {
              "config": {
                "expression": "{this.custom.ReasonGroupSet}\r\n\u0026\u0026\r\n{this.custom.ReasonSet}\r\n\u0026\u0026\r\n{this.custom.DefectsSet}"
              },
              "type": "expr"
            }
          },
          "props.primary": {
            "binding": {
              "config": {
                "path": "this.props.enabled"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "text": "Save"
        },
        "type": "ia.input.button"
      },
