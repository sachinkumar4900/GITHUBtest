            "onActionPerformed": {
              "config": {
                "script": "\tself.getSibling(\"Reason TextField\").props.text \u003d self.view.custom.ReasonData[0].Name\n\tself.getSibling(\"Consecutive NumericEntryField\").props.value \u003d self.view.custom.ReasonData[0].ConsecutiveDefects\n\tself.getSibling(\"Defects NumericEntryField\").props.value \u003d self.view.custom.ReasonData[0].DefectsPerHour\n\tself.getSibling(\"Reason Group Dropdown\").props.value \u003d self.view.custom.ReasonData[0].ProductionLineTypeID"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
